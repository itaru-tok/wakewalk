{
  "name": "wakewalk",
  "version": "0.0.1",
  "private": true,
  "main": "index.js",
  "scripts": {
    "dev": "expo start --tunnel -c",
    "dev:client": "expo start --dev-client --tunnel",
    "ios": "expo run:ios --device",
    "android": "expo run:android",
    "web": "expo start --web",
    "typecheck": "tsc --noEmit",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "icons": "node scripts/generate-icons.js",
    "eas": "eas",
    "eas:login": "eas login",
    "eas:build:dev:ios": "eas build --platform ios --profile development",
    "eas:build:preview:ios": "eas build --platform ios --profile preview",
    "eas:build:production:ios": "eas build --platform ios --profile production",
    "eas:submit:ios": "eas submit --platform ios",
    "eas:build:production:ios:auto-submit": "eas build --platform ios --profile production --auto-submit",
    "prebuild:ios": "npx expo prebuild -p ios"
  },
  "dependencies": {
    "@expo-google-fonts/comfortaa": "^0.4.1",
    "@expo/ui": "0.2.0-beta.2",
    "@expo/vector-icons": "^15.0.2",
    "@react-native-async-storage/async-storage": "~2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-native-community/slider": "^5.0.1",
    "@types/react": "~19.1.12",
    "dotenv": "^17.2.2",
    "expo": "~54.0.2",
    "expo-av": "^16.0.7",
    "expo-constants": "~18.0.8",
    "expo-dev-client": "~6.0.12",
    "expo-font": "^14.0.8",
    "expo-glass-effect": "^0.1.2",
    "expo-haptics": "^15.0.7",
    "expo-linear-gradient": "~15.0.7",
    "expo-linking": "~8.0.8",
    "expo-mesh-gradient": "~0.4.7",
    "expo-notifications": "~0.28.9",
    "expo-router": "~6.0.1",
    "expo-sensors": "~15.0.7",
    "nativewind": "^4.0.36",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-css-interop": "^0.2.0",
    "react-native-google-mobile-ads": "^15.7.0",
    "react-native-modal-datetime-picker": "^14.0.0",
    "react-native-purchases": "^9.5.0",
    "react-native-purchases-ui": "^9.5.0",
    "react-native-reanimated": "~4.1.0",
    "react-native-safe-area-context": "5.6.1",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-web": "^0.21.1",
    "react-native-worklets": "0.5.1",
    "swift-ui": "link:@expo/ui/swift-ui"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.4",
    "@expo/config-plugins": "^54.0.1",
    "babel-preset-expo": "^54.0.2",
    "react-native-svg-transformer": "^1.5.1",
    "tailwindcss": "^3.4.7",
    "typescript": "~5.9.2"
  }
}
